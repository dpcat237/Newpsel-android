<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dpcat237.nps"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.dpcat237.nps.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.dpcat237.nps.FeedsActivity"
            android:parentActivityName="com.dpcat237.nps.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.dpcat237.nps.MainActivity" />
        </activity>
        <activity
            android:name="com.dpcat237.nps.FeedActivity"
            android:label="@string/create_feed"
            android:parentActivityName="com.dpcat237.nps.FeedsActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.dpcat237.nps.FeedsActivity" />
        </activity>
        <activity
            android:name="com.dpcat237.nps.JsonActivity"
            android:parentActivityName="com.dpcat237.nps.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.dpcat237.nps.MainActivity" />
        </activity>
        
        <service
            android:name=".service.FeedService"
            android:enabled="true" />
    </application>

</manifest>