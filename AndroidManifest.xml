<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dpcat237.nps"
    android:versionCode="0115"
    android:versionName="0.1.15" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.dpcat237.nps.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:label="@string/label_st"
            android:name="com.dpcat237.nps.SettingsActivity"
            android:parentActivityName="com.dpcat237.nps.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.dpcat237.nps.MainActivity" />
        </activity>
        <activity
            android:label="@string/label_signup"
            android:name="com.dpcat237.nps.SignUpActivity"
            android:parentActivityName="com.dpcat237.nps.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.dpcat237.nps.MainActivity" />
        </activity>
        <activity
            android:label="@string/label_signin"
            android:name="com.dpcat237.nps.SignInActivity"
            android:parentActivityName="com.dpcat237.nps.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.dpcat237.nps.MainActivity" />
        </activity>
        <activity
            android:label="@string/label_add_feed"
            android:name="com.dpcat237.nps.AddFeedActivity"
            android:parentActivityName="com.dpcat237.nps.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.dpcat237.nps.MainActivity" />
        </activity>
        <activity
            android:label="@string/label_labels_list"
            android:name="com.dpcat237.nps.LabelsActivity"
            android:parentActivityName="com.dpcat237.nps.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.dpcat237.nps.MainActivity" />
        </activity>
        <activity
            android:label="@string/label_labels_create"
            android:name="com.dpcat237.nps.CreateLabelActivity"
            android:parentActivityName="com.dpcat237.nps.LabelsActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.dpcat237.nps.LabelsActivity" />
        </activity>
        <activity
            android:name="com.dpcat237.nps.ItemsActivity"
            android:parentActivityName="com.dpcat237.nps.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.dpcat237.nps.MainActivity" />
        </activity>
        <activity
            android:name="com.dpcat237.nps.ItemActivity"
            android:parentActivityName="com.dpcat237.nps.ItemsActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.dpcat237.nps.ItemsActivity" />
        </activity>
        <activity
            android:name="com.dpcat237.nps.AboutActivity"
            android:label="@string/about_label"
            android:parentActivityName="com.dpcat237.nps.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.dpcat237.nps.MainActivity" />
        </activity>
        
        <service
            android:name=".service.FeedService"
            android:enabled="true" />
    </application>

</manifest>